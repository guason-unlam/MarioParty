<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-mapping>
	<class name="juego.tablero.MiniJuego" table="MiniJuego">
		<id column="id" name="id" type="int" />
		<property column="nombre" name="nombre" type="string" />
		<property column="descripcion" name="descripcion"
			type="string" />
	</class>
</hibernate-mapping>
